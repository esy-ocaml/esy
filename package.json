{
  "name": "esy",
  "version": "0.2.11",
  "description": "Package builder for esy",
  "license": "MIT",
  "esy": {
    "build": [
      ["dune", "build", "-j", "4"],
      [
        "dune",
        "build",
        "esy/bin/esyCommand.exe",
        "esy-build-package/bin/esyBuildPackageCommand.exe"
      ]
    ],
    "install": [
      ["esy-installer", "./esy-build-package.install"],
      ["esy-installer", "./esy.install"]
    ],
    "buildsInSource": "_build"
  },
  "scripts": {
    "install:esy": "esy install",
    "build": "npm run build:esy && npm run build:copy-frs && npm run build:postinstall-esy-bash",
    "build:esy": "esy build",
    "build:copy-frs": "node scripts/build/copy-fastreplacestring.js",
    "build:postinstall-esy-bash": "cd node_modules/esy-bash && npm install",
    "release:make-platform-package": "node ./scripts/build/make-platform-package.js",
    "test:unit": "esy b dune runtest test",
    "test:e2e": "jest --runInBand test-e2e"
  },
  "resolutions": {
    "@esy-ocaml/esy-installer": "0.0.1",
    "@opam/merlin-extend": "0.3",
    "@opam/angstrom": "github:esy-ocaml/angstrom#b3a125f"
  },
  "dependencies": {
    "@esy-ocaml/esy-installer": "^0.0.0",
    "@esy-ocaml/reason": "~3.2.0",
    "@opam/bos": "^0.2.0",
    "@opam/angstrom": "*",
    "@opam/cmdliner": "^1.0.2",
    "@opam/cudf": "0.9",
    "@opam/dose3": "*",
    "@opam/dune": "*",
    "@opam/ezgzip": "0.2.1",
    "@opam/fmt": "^0.8.4",
    "@opam/fpath": "^0.7.2",
    "@opam/lambda-term": "^1.11.0",
    "@opam/logs": "^0.6.2",
    "@opam/lwt": "*",
    "@opam/lwt_ppx": "*",
    "@opam/menhir": "20171013",
    "@opam/opam-core": "*",
    "@opam/opam-file-format": "*",
    "@opam/opam-format": "*",
    "@opam/opam-state": "*",
    "@opam/ppx_deriving": "*",
    "@opam/ppx_deriving_yojson": "*",
    "@opam/ppx_inline_test": "*",
    "@opam/ppx_let": "*",
    "@opam/re": "^1.8.0",
    "@opam/tar": "0.9.0",
    "@opam/yojson": "*",
    "esy-solve-cudf": "^0.1.6",
    "fastreplacestring": "esy-ocaml/FastReplaceString#95f408b"
  },
  "peerDependencies": {
    "ocaml": "~4.6.6"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*",
    "@opam/utop": "*",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "del": "^3.0.0",
    "esy-bash": "^0.1.23",
    "flow-bin": "^0.77.0",
    "fs-extra": "^7.0.0",
    "jest-cli": "^23.4.1",
    "klaw": "^2.1.1",
    "minimatch": "^3.0.4",
    "ocaml": "~4.6.6",
    "outdent": "^0.3.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "super-resolve": "^1.0.0",
    "tar": "^4.4.4",
    "tar-fs": "^1.16.0",
    "tmp": "^0.0.33"
  }
}
