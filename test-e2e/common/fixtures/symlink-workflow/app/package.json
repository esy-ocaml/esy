{
  "name": "app",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "build": [
      [
        "cp",
        "#{self.original_root /}app.ml",
        "#{self.target_dir /}app.ml"
      ],
      [
        "ocamlopt",
        "-o",
        "#{self.target_dir / self.name}.exe",
        "#{self.target_dir /}app.ml"
      ]
    ],
    "install": [
      "cp $cur__target_dir/$cur__name.exe $cur__bin/$cur__name"
    ]
  },
  "dependencies": {
    "dep": "link:../dep",
    "another-dep": "link:../another-dep",
    "ocaml": "esy-ocaml/ocaml#6aacc05"
  }
}
