{
  "name": "with-linked-dep-_build",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "build": [
      [
        "bash",
        "-c",
        "echo '#!/bin/bash\necho #{self.name}' > #{self.target_dir / self.name}"
      ],
      "chmod +x $cur__target_dir/$cur__name"
    ],
    "install": [
      "cp $cur__target_dir/$cur__name $cur__bin/$cur__name"
    ]
  },
  "dependencies": {
    "dep": "*"
  }
}
