{
  "name": "no-deps-_build",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "buildsInSource": "_build",
    "build": [
      "mkdir -p _build",
      [
        "bash",
        "-c",
        "echo 'print_endline \"#{self.name}\"' > ./_build/test.ml"
      ],
      [
        "ocamlopt",
        "-o",
        "./_build/#{self.name}.exe",
        "./_build/test.ml"
      ]
    ],
    "install": [
      "cp ./_build/#{self.name}.exe $cur__bin/$cur__name"
    ]
  }
}
