{
  "name": "custom-prefix",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "build": [
      [
        "cp",
        "#{self.original_root /}test.ml",
        "#{self.target_dir /}test.ml"
      ],
      [
        "ocamlopt",
        "-o",
        "#{self.target_dir / self.name}.exe",
        "#{self.target_dir /}test.ml"
      ]
    ],
    "install": [
      [
        "cp",
        "#{self.target_dir / self.name}.exe",
        "#{self.bin / self.name}"
      ]
    ]
  },
  "dependencies": {
    "ocaml": "~4.6.1"
  }
}
