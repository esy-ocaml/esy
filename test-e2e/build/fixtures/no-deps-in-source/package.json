{
  "name": "no-deps-in-source",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "buildsInSource": true,
    "build": [
      [
        "bash",
        "-c",
        "echo 'print_endline \"#{self.name}\"' > test.ml"
      ],
      [
        "ocamlopt",
        "-o",
        "#{self.name}.exe",
        "test.ml"
      ]
    ],
    "install": [
      "cp ./$cur__name.exe $cur__bin/$cur__name"
    ]
  }
}
