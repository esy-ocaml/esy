{
  "name": "dep",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "build": [
      [
        "bash",
        "-c",
        "echo \"#!/bin/bash\necho $cur__name\" > $cur__target_dir/$cur__name"
      ],
      "chmod +x $cur__target_dir/$cur__name"
    ],
    "install": [
      "cp $cur__target_dir/$cur__name $cur__install/lib"
    ],
    "exportedEnv": {
      "PATH": {
        "val": "#{self.lib : $PATH}",
        "scope": "global"
      }
    }
  },
  "_resolved": "http://sometarball.gz"
}
