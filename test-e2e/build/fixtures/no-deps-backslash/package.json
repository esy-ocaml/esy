{
  "name": "no-deps-backslash",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "build": [
      [
        "bash",
        "-c",
        "echo 'print_endline \"\\\\\\\\ #{self.name} \\\\\\\\\"' > #{self.target_dir /}test.ml"
      ],
      [
        "ocamlopt",
        "-o",
        "#{self.target_dir / self.name}.exe",
        "#{self.target_dir /}test.ml"
      ]
    ],
    "install": [
      "cp $cur__target_dir/$cur__name.exe $cur__bin/$cur__name"
    ]
  }
}
