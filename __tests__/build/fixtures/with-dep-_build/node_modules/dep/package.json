{
  "name": "dep",
  "version": "1.0.0",
  "license": "MIT",
  "esy": {
    "buildsInSource": "_build",
    "build": [
      "mkdir _build",
      "echo '#!/bin/bash\necho $cur__name' > ./_build/$cur__name",
      "chmod +x ./_build/$cur__name"
    ],
    "install": [
      "cp ./_build/$cur__name $cur__bin/$cur__name"
    ]
  },
  "_resolved": "http://sometarball.gz"
}
